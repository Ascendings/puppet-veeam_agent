{
  "name": "puppet-veeam_agent",
  "version": "0.1.0",
  "author": "Ascendings",
  "summary": "Installs and configures the Veeam Endpoint Backup Agent for Linux package",
  "license": "BSD-2-Clause",
  "source": "https://github.com/Ascendings/puppet-veeam_agent",
  "project_page": "https://github.com/ascendings/puppet-veeam_agent",
  "issues_url": "https://github.com/ascendings/puppet-veeam_agent/issues",
  "dependencies": [
